 */

public class FindLineTest {
    /** Stream for redirecting standard output. */
    private ByteArrayOutputStream outContent;

    /** Timeout for our tests. */
    private static final int TEST_TIMEOUT = 1000;

    /** Pre-computed search results to test against. */
    private static final Map<FindWordInStringInput, String[]> PRECOMPUTED_RESULTS =
        new HashMap<FindWordInStringInput, String[]>();

    /**
     * Test searches that should return results.
     */
    @Test(timeOut = TEST_TIMEOUT)
    public void testFindWordInString() {
        for (Map.Entry<FindWordInStringInput, String[]> precomputedResult : PRECOMPUTED_RESULTS
                .entrySet()) {

            /* Redirect standard out. */
            outContent = new ByteArrayOutputStream();
            System.setOut(new PrintStream(outContent));

            FindWordInStringInput input = precomputedResult.getKey();
            String[] expectedOutput = precomputedResult.getValue();
            FindLine.printMatchingLines(input.searchWord, input.contents);
            String[] output = outContent.toString().split("\\R");

            /* Reset standard out. */
            System.setOut(new PrintStream(new FileOutputStream(FileDescriptor.out)));

            /* Ensure that output is the expected length. */
            Assert.assertEquals(output.length, expectedOutput.length,
                    "Wrong line count for " + input);

            /* Check each line of the output. */
            for (int outputLine = 0; outputLine < output.length; outputLine++) {
                Assert.assertTrue(expectedOutput[outputLine].equals(output[outputLine]),
                        expectedOutput[outputLine] + " does not equal " + output[outputLine]);
            }
        }
    }

    /**
     * Class storing testing input to findWordInString.
     */
    @SuppressWarnings("checkstyle:visibilitymodifier")
    public static class FindWordInStringInput {

        /** String to search for. */
        public final String searchWord;

        /** Input file to load string from. Relative to the resources directory. */
        public final String contents;

        /** Original file path. */
        public final String contentsPath;

        /**
         * Create a new set of testing inputs for findWordInString.
         *
         * @param setSearchWord the search word to look for
         * @param setContentsPath resource-related path to file with text contents
         */
		public FindWordInStringInput(final String setSearchWord, final String setContentsPath) {
            this.searchWord = setSearchWord;
            this.contentsPath = setContentsPath;
            String contentsFilePath;
			try {
				contentsFilePath = FindLineTest.class.getClassLoader().getResource(contentsPath).getFile();
				contentsFilePath = new URI(contentsFilePath).getPath();
                File contentsFile = new File(contentsFilePath);
                Scanner contentsScanner = new Scanner(contentsFile, "UTF-8");
                this.contents = contentsScanner.useDelimiter("\\A").next();
                contentsScanner.close();
			} catch (Exception e) {
				throw new InvalidParameterException("Bad file path" + e);
			}
        }

        @Override
        public final String toString() {
            return "FindWordInStringInput [searchWord=" + searchWord + ", contentsPath="
                + contentsPath + "]";
        }
    }

